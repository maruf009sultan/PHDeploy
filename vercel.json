{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/search",
      "dest": "/api/search.js"
    },
    {
      "src": "/api/callback",
      "dest": "/api/callback.js",
      "methods": ["POST"]
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ]
}
